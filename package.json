{
	"name": "chat-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"react-jss": "^8.6.1",
		"react-redux": "^5.0.7",
		"react-scripts": "2.0.3",
		"redux": "^4.0.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"parser": "babel-eslint",
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"plugins": [
			"react"
		],
		"rules": {
			"indent": [
				"error",
				"tab"
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"jsx-quotes": [
				"error",
				"prefer-single"
			],
			"comma-dangle": [
				"error",
				"never"
			]
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
