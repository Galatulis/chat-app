    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,html,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
